strict digraph {
	 rankdir=LR
	 node [shape = none, label = " ", width = 0, height = 0]; qd;
	 node [label = "\N", width = 0.5, height = 0.5];
	 node [shape = doublecircle]; q0;
	 node [shape = doublecircle]; q4;
	 node [shape = circle];
	 q4 -> q2 [label = "c"]
	 q0 -> q2 [label = "c"]
	 q0 -> q1 [label = "b"]
	 q0 -> q0 [label = "a"]
	 q3 -> q4 [label = "c"]
	 q1 -> q0 [label = "b"]
	 q1 -> q1 [label = "a"]
	 q2 -> q3 [label = "c"]
	 qd -> q0
}